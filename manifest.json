{
  "name": "Github navigator profile links",
  "version": "1.0.0",
  "author": "yubaoquan",
  "manifest_version": 2,
  "description": "Add profile links as a dropdown menu to github navigator.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [

  ],
  "content_scripts": [
   {
     "matches": [
       "http://*/*",
       "https://*/*"
       ],

     "js": ["jquery.min.js", "myscript.js"]
   }
 ]
}
